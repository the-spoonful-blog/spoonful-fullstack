    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "morgan": "^1.10.0",
    "parcel": "^2.8.3",
    "parcel-bundler": "^1.8.1",
